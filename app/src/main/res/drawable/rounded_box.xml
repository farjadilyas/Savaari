<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="?attr/backgroundColor"
    android:enterFadeDuration="1400"
    android:exitFadeDuration="1500">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle"
            android:shadowColor="?attr/textColor">
            <stroke android:width="2dp" android:color="?attr/backgroundColor" />
            <corners android:radius="10dp" />
            <solid android:color="?attr/backgroundColor" />
        </shape>
    </item>
</ripple>
