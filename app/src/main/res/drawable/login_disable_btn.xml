<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="?attr/backgroundColor"
    android:enterFadeDuration="400"
    android:exitFadeDuration="500">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <stroke android:width="2dp" android:color="?attr/deselectedForegroundColor" />
            <corners android:radius="6dp" />
            <solid android:color="?attr/deselectedForegroundColor" />
        </shape>
    </item>
</ripple>